        <div id="container_popUp_calendar">
            <div class="popUp_calendar">
                <div class="calendar-header">
                    <span class="month-picker" id="month-picker">February</span>
                    <div class="year-picker">
                        <span class="year-change" id="prev-year">
                            <i class="fa-solid fa-chevron-left chevron-left"></i>
                        </span>
                        <span id="year">2022</span>
                        <span class="year-change" id="next-year">
                            <i class="fa-solid fa-chevron-right chevron-right"></i>
                        </span>
                    </div>
                </div>
                <div class="calendar-body">
                    <div class="calendar-week-day">
                        <div>Dom</div>
                        <div>Seg</div>
                        <div>Ter</div>
                        <div>Qua</div>
                        <div>Qui</div>
                        <div>Sex</div>
                        <div>Sab</div>
                    </div>
                    <div class="calendar-days"></div>
                </div>
                <div class="month-list"></div>
            </div>
        </div>

        <div id="container_popUp_cancel">
            <div class="popUp_cancel">
                <div>
                    <div id="popUp-cancel_state1">
                        <div>
                            <label for="reason_cancellation">Diga o motivo pelo qual você deseja cancelar</label>
                            <img id="close_popUp_cancel" src="images/default/close_icon.svg" alt="close">
                        </div>
                        
                        <textarea name="reason_cancellation" id="reason_cancellation" rows="4"
                            class="form-control"></textarea>
                        <div class="popUp_go">
                            <p>Etapa 1/3</p>
                            <button name="next" class="go">Avançar</button>
                        </div>
                    </div>

                    <div id="popUp-cancel_state2">
                        <div>
                            <i class="fa-solid fa-circle-chevron-left arrow-left"></i>
                        </div>
                        <div class="popUp_flex">
                            <img class="avaliation-img" src="images/my-account/reservations/cancel_reservations1.png">
                            <div>
                                <label for="comment1">Avalie o espaço</label>

                                <ul class="rating">
                                    <li><img class="star" src="images/search-spaces/search/star.png" alt="estrela"></li>
